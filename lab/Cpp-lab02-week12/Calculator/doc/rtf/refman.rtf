00031 {\cf18 void} Dialog::onButtonGroupClicked(QAbstractButton* btn) \{\par
00032     {\cf20 // Print in terminal}\par
00033     qInfo() << btn->text();\par
00034 \par
00035     {\cf18 float} val = this->ui->lineEdit->text().toFloat();\par
00036 \par
00037     {\cf20 // if the number pressed, print}\par
00038 \par
00042     QString name = btn->text();\par
00043 \par
00044     {\cf20 // \'B8\'F9\'BE\'DD\'B0\'B4\'C5\'A5\'B5\'C4\'B5\'E3\'BB\'F7\'A3\'AC\'B4\'A6\'C0\'ED\'B2\'BB\'CD\'AC\'B5\'C4\'C2\'DF\'BC\'AD}\par
