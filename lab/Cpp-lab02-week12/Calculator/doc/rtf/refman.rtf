00046     {\cf19 if} ((name >= {\cf22 "0"} && name <= {\cf22 "9"}) || name == {\cf22 "."}) \{\par
00047         {\cf20 // \'D2\'BB\'BF\'AA\'CA\'BC\'CF\'D4\'CA\'BE\'C1\'E3\'A3\'AC\'CA\'E4\'C8\'EB\'D2\'BB\'B8\'F6\'CA\'FD}\par
00048         {\cf19 if}(ui->lineEdit->text() == {\cf22 "0"} && name != {\cf22 "."}) \{\par
00049             ui->lineEdit->clear();\par
00050         \}\par
00051 \par
00052         {\cf20 // \'C8\'E7\'B9\'FB\'B5\'E3\'BB\'F7\'CA\'FD\'D7\'D6\'BC\'FC\'A3\'AC\'CA\'E4\'C8\'EB\'BF\'F2\'D3\'D0\'CA\'FD\'BE\'DD\'A3\'AC\'B6\'F8\'C7\'D2\'B1\'ED\'B4\'EF\'CA\'BD\'BF\'F2\'D6\'BB\'D3\'D0\'C1\'BD\'B8\'F6\'CA\'FD\'BE\'DD\'A3\'AC\'D6\'D8\'D6\'C3\'CA\'E4\'C8\'EB\'B5\'C4}\par
00053         {\cf19 if}(this->prevBtn == {\cf22 "+"} || this->prevBtn == {\cf22 "-"} || this->prevBtn == {\cf22 "*"} || this->prevBtn == {\cf22 "/"} || this->prevBtn == {\cf22 "="}) \{\par
00054             this->ui->lineEdit->clear();\par
00055         \}\par
00056         this->ui->lineEdit->insert(name);\par
00057     \}\par
00058     {\cf19 else} {\cf19 if} (name == {\cf22 "+"}) \{\par
00059         {\cf19 if}(this->vectorOfNumbersAndSigns[2].isNull()) \{\par
00060             {\cf20 // \'B0\'D1 lhs \'CA\'FD\'D7\'D6\'BA\'CD\'B2\'D9\'D7\'F7\'B7\'FB\'B4\'E6\'C6\'F0\'C0\'B4}\par
00061             this->vectorOfNumbersAndSigns[0] = val;\par
00062             this->vectorOfNumbersAndSigns[1] = {\cf22 "+"};\par
00063         \}\par
00064     \}\par
00065     {\cf19 else} {\cf19 if} (name == {\cf22 "-"}) \{\par
00066         {\cf19 if}(this->vectorOfNumbersAndSigns[2].isNull()) \{\par
00067             {\cf20 // \'B0\'D1 lhs \'CA\'FD\'D7\'D6\'BA\'CD\'B2\'D9\'D7\'F7\'B7\'FB\'B4\'E6\'C6\'F0\'C0\'B4}\par
00068             this->vectorOfNumbersAndSigns[0] = val;\par
00069             this->vectorOfNumbersAndSigns[1] = {\cf22 "-"};\par
00070         \}\par
00071     \}\par
00072     {\cf19 else} {\cf19 if} (name == {\cf22 "*"}) \{\par
00073         {\cf19 if}(this->vectorOfNumbersAndSigns[2].isNull()) \{\par
00074             {\cf20 // \'B0\'D1 lhs \'CA\'FD\'D7\'D6\'BA\'CD\'B2\'D9\'D7\'F7\'B7\'FB\'B4\'E6\'C6\'F0\'C0\'B4}\par
00075             this->vectorOfNumbersAndSigns[0] = val;\par
00076             this->vectorOfNumbersAndSigns[1] = {\cf22 "*"};\par
00077         \}\par
00078     \}\par
00079     {\cf19 else} {\cf19 if} (name == {\cf22 "/"}) \{\par
00080         {\cf19 if}(this->vectorOfNumbersAndSigns[2].isNull()) \{\par
00081             {\cf20 // \'B0\'D1 lhs \'CA\'FD\'D7\'D6\'BA\'CD\'B2\'D9\'D7\'F7\'B7\'FB\'B4\'E6\'C6\'F0\'C0\'B4}\par
00082             this->vectorOfNumbersAndSigns[0] = val;\par
00083             this->vectorOfNumbersAndSigns[1] = {\cf22 "/"};\par
00084         \}\par
00085     \}\par
00086     {\cf19 else} {\cf19 if} (name == {\cf22 "="}) \{\par
00087         this->vectorOfNumbersAndSigns[2] = val;\par
00088         this->vectorOfNumbersAndSigns[3] = {\cf22 "="};\par
00089         {\cf19 if}(this->vectorOfNumbersAndSigns[1] == {\cf22 "+"}) \{\par
00090            this->vectorOfNumbersAndSigns[4] = this->vectorOfNumbersAndSigns[0].toFloat() + this->vectorOfNumbersAndSigns[2].toFloat();\par
00091         \}\par
00092         {\cf19 else} {\cf19 if}(this->vectorOfNumbersAndSigns[1] == {\cf22 "-"}) \{\par
00093             this->vectorOfNumbersAndSigns[4] = this->vectorOfNumbersAndSigns[0].toFloat() - this->vectorOfNumbersAndSigns[2].toFloat();\par
00094         \}\par
00095         {\cf19 else} {\cf19 if}(this->vectorOfNumbersAndSigns[1] == {\cf22 "*"}) \{\par
00096             this->vectorOfNumbersAndSigns[4] = this->vectorOfNumbersAndSigns[0].toFloat() * this->vectorOfNumbersAndSigns[2].toFloat();\par
00097         \}\par
00098         {\cf19 else} {\cf19 if}(this->vectorOfNumbersAndSigns[1] == {\cf22 "/"}) \{\par
00099             this->vectorOfNumbersAndSigns[4] = this->vectorOfNumbersAndSigns[0].toFloat() / this->vectorOfNumbersAndSigns[2].toFloat();\par
00100         \}\par
00101 \par
00102 {\cf20 //        this->ui->lineEdit->setText(this->vectorOfNumbersAndSigns[4].toString());}\par
00103         this->ui->lineEdit->setText(QString::asprintf({\cf22 "%.2f"}, this->vectorOfNumbersAndSigns[4].toFloat()));\par
00104         this->vectorOfNumbersAndSigns.clear();\par
00105     \}\par
00106     {\cf19 else} {\cf19 if} (name == {\cf22 "C"}) \{\par
00107         this->ui->lineEdit->clear();\par
00108         this->ui->expLineEdit->clear();\par
00109         this->vectorOfNumbersAndSigns.clear();\par
00110     \}\par
00111     {\cf19 else} {\cf19 if} (name == {\cf22 "CE"}) \{\par
00112         this->ui->lineEdit->clear();\par
00113         this->vectorOfNumbersAndSigns.pop_back();\par
00114     \}\par
00115     {\cf19 else} {\cf19 if} (name == {\cf22 "DEL"}) \{\par
00116         this->ui->lineEdit->setCursorPosition(ui->lineEdit->cursorPosition() - 1);\par
00117         this->ui->lineEdit->del();\par
00118     \}\par
00119 \par
00120     {\cf20 // Display Sign and Number}\par
